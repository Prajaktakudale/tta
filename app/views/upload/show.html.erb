<%= render :partial => '/home/header.html.erb' %>
<table border="1" style="text-align: left; width: 100%; padding-left: 20%; padding-right: 20%">
  <thead>
  <tr>
    <th></th>
    <th><h2 id="notice"><%= flash[:notice] %></h2></th>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td><b>Name:</b></td>
    <td><%= @project.name %></td>
  </tr>
  <tr>
    <td><b>Sub Project name:</b></td>
    <td><%= @sub_project.name %></td>
  </tr>
  <tr>
    <td><b>CI Job name:</b></td>
    <td><%= @project_meta.ci_job_name %></td>
  </tr>
  <tr>
    <td><b>Test Category:</b></td>
    <td><%= @project_meta.test_category %></td>
  </tr>
  <tr>
    <td><b>Test Report Type:</b></td>
    <td><%= @project_meta.test_report_type %></td>
  </tr>
  <tr>
    <td><b>OS Tested On:</b></td>
    <td><%= @project_meta.os_name %></td>
